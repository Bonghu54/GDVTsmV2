@page
@model GDVTsmV3.Views.Usuarios.IndexModel

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Usuario[0].Nombre_Usuario)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Usuario[0].Correo_Electronico)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Usuario[0].Contrasena)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Usuario) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nombre_Usuario)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Correo_Electronico)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Contrasena)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.Usuario_Id">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.Usuario_Id">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.Usuario_Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
</body>
</html>
